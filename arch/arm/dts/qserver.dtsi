	gic: interrupt-controller@38000000 {
		compatible = "arm,gic-400";
		reg = <0x0 0x38001000 0 0x1000>,
		      <0x0 0x38002000 0 0x2000>;
		#address-cells = <0>;
		#interrupt-cells = <3>;
		interrupt-controller;
		interrupts = <GIC_PPI 9 (GIC_CPU_MASK_SIMPLE(8) | IRQ_TYPE_LEVEL_HIGH)>;
	};
